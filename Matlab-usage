>> A=[1 2 3;4 5 6;7 8 9]

A =

     1     2     3
     4     5     6
     7     8     9

>> A(:,1:2:3)

ans =

     1     3
     4     6
     7     9
>> A

A =

     1     2     3
     4     5     6
     7     8     9

>> A(1:2:1:3)

ans =

     1     4     7

>> linspace(1, 2, 11)

ans =

    1.0000    1.1000    1.2000    1.3000    1.4000    1.5000    1.6000    1.7000    1.8000    1.9000    2.0000


